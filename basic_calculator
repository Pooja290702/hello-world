def add(a,b):
    return a+b
def subtract(a,b):
    return a-b
def multiply(a,b):
    return a*b
def divide(a,b):
    if a!=0:
        return a//b
    else: 
        return 'cannot divide by zero'
a=int(input('a:'))
b=int(input('b:'))
print(add(a,b))
print(subtract(a,b))
print(multiply(a,b))
print(divide(a,b))
